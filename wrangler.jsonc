{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "fluxly-ccs",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-05",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "ALLOWED_ORIGINS": "https://fluxly.app",
    "CORS_ALLOW_LOCALHOST": "false",
    "CORS_ENABLE_LOGGING": "false"
  },
  "observability": {
    "enabled": true
  }
}
